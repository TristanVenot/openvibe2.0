      <column type="guint64"/>
      <!-- column-name name -->
      <column type="gchararray"/>
      <!-- column-name default-value -->
      <column type="gchararray"/>
      <!-- column-name value -->
      <column type="gchararray"/>
    </columns>
  </object>
  <object class="GtkListStore" id="liststore-pipelines">
    <columns>
