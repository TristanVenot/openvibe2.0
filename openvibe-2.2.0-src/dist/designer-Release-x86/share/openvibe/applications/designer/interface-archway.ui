      <column type="guint64"/>
      <!-- column-name pipeline-hex-id -->
      <column type="gchararray"/>
      <!-- column-name pipeline-description -->
      <column type="gchararray"/>
      <!-- column-name pipeline-has-configuration -->
      <column type="gboolean"/>
    </columns>
